#include <stdio.h>
#include <string.h>

int main(void){
 char palavra1[20], palavra2[20], palavra3[20]; 
 
 scanf("%s", palavra1);
 scanf("%s", palavra2);
 scanf("%s", palavra3);
 
 if(strcmp("vertebrado",palavra1) == 0){

 if(strcmp("ave",palavra2) == 0){

 if(strcmp("carnivoro",palavra3) == 0) {

 printf("aguia\n");
 }
 if(strcmp("onivoro",palavra3) == 0) {

 printf("pomba\n");

 }
 }
 if(strcmp("mamifero",palavra2) == 0){

 if(strcmp("onivoro",palavra3) == 0){

 printf("homem\n");
 }

 if(strcmp("herbivoro",palavra3) == 0){

 printf("vaca\n");
 }
 }
 }

 if(strcmp("invertebrado",palavra1) == 0){

 if(strcmp("inseto",palavra2) == 0) {
 if(strcmp("hematofago",palavra3) == 0) {
 printf("pulga\n");
 }
 if(strcmp("herbivoro",palavra3) == 0) {
 printf("lagarta\n");
 }
 }
 if(strcmp("anelideo",palavra2) == 0){

 if(strcmp("hematofago",palavra3) == 0) {
 printf("sanguessuga\n");
 }

 if(strcmp("onivoro",palavra3) == 0) {
 printf("minhoca\n");
 }
 }
 }
 return 0;
}
