#include <stdio.h>

int main() {
 
int mesdoano;

scanf("%d",&mesdoano);

if (mesdoano==1)
printf("January\n");
else

if (mesdoano==2)

printf("February\n");

else

if (mesdoano==3)

printf("March\n");

else
if (mesdoano==4)

printf("April\n");
else

if (mesdoano==5)

printf("May\n");

else

if (mesdoano==6)

printf("June\n");

else

if (mesdoano==7)
printf("July\n");

else

if (mesdoano==8)

printf("August\n");

else
if (mesdoano==9)

printf("September\n");

else

if (mesdoano==10)

printf("October\n");

else

if (mesdoano==11)

printf("November\n");

else

if (mesdoano==12)
printf("December\n");

return 0;
}
